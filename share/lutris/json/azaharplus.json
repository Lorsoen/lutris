{
    "human_name": "AzaharPlus",
    "description": "3DS emulator with .3DS support",
    "platforms": ["Nintendo 3DS"],
    "require_libs": [],
    "runner_executable": "azaharplus/azaharplus-qt",
    "runnable_alone": true,
    "game_options": [
        {
            "option": "main_file",
